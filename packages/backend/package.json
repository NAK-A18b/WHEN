{
  "name": "when-backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/app/index.js",
  "scripts": {
    "invoke:app": "npm run build && node build/app/index.js",
    "invoke:update-timetable": "npm run build && sls invoke local --function updateTimetable",
    "invoke:update-hvv-request": "npm run build && sls invoke local --function hvvRequest",
    "invoke:update-create-user": "npm run build && sls invoke local --function createUser",
    "build": "babel --presets es2015 -d build/ src",
    "deploy": "sls deploy",
    "db:setup": "serverless dynamodb install",
    "db:drop": "rm -rf ./.dynamodb",
    "db:start": "serverless dynamodb start",
    "sls:offline": "sls offline --noTimeout"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "request": "2.88.0",
    "selenium-webdriver": "4.0.0-alpha.4",
    "when-aws": "file:../aws"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "serverless-dynamodb-local": "^0.2.38",
    "serverless-offline": "^5.10.1"
  }
}
