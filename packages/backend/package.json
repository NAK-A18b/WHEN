{
  "name": "when-backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/app/index.js",
  "scripts": {
    "invoke:app": "npm run build && node build/app/index.js",
    "invoke:update-timetable": "sls invoke local --function updateTimetable",
    "invoke:update-hvv-request": "npm run build && sls invoke local --function hvvRequest",
